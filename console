<?php

require('bootstrap.php');

$app = new Commandr\Core\Application(
new Commandr\Core\Input,
new Commandr\Core\Output,
new Commandr\Core\Dialog,
'Commandr Console Application',
'1.0');

$config = new Commandr\Core\Config(require('config/config.php'));

$app->setConfig($config);

$app->addCommands($config->get('commands'));

echo $app->match()->run(new Commandr\Core\Output());